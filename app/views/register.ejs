    <!-- register-area -->
    <div class="register-area" style="background-color: #272727;">
        <div class="container">

            <div class="col-md-6">
                <div class="box-for overflow">
                    <div class="col-md-12 col-xs-12 register-blocks">
                        <h2>สมัครสมาชิก </h2>
                       <form name='register' method='POST' action='/register/<%=pages%>'>
                            <div class="form-group">
                                <label>ชื่อ</label>
                                <input type="text" class="form-control" name='name'>
                            </div>
                            <div class="form-group">
                                <label>นามสกุล</label>
                                <input type="text" class="form-control" name='surname'>
                            </div>
                            <div class="form-group">
                                <label >อีเมลล์</label>
                                <input type="text" class="form-control" name='email'>
                            </div>
                            <div class="form-group">
                                    <label for="name">ชื่อผู้ใช้งาน</label>
                                    <input type="text" class="form-control" name='username'>
                                </div>
                            <div class="form-group">
                                <label for="password">รหัสผ่าน</label>
                                <input type="password" class="form-control" name='password'>
                            </div>
                            <div class="form-group">
                                <label for="password">วัน เดือน ปี เกิด</label>
                                <input type="date" class="form-control" name='dateofbirth'>
                            </div>
                            <div class="form-group">
                                <label for="password">เบอร์โทรศัพท์</label>
                                <input type="text" class="form-control" name='phone'>
                            </div>
                            <div class="text-center">
                                <button type="submit" value='register' class="btn btn-default" >สมัครสมาชิก</button>
                            </div>

                        </form>
                    </div>
                </div>
            </div>

            <div class="col-md-6">
                <div class="box-for overflow">
                    <div class="col-md-12 col-xs-12 login-blocks">
                        <h2>ลงชื่อเข้าใช้ </h2>
                        <form action="/login/<%=pages%>" method='POST'>
                            <div class="form-group">
                                <label for="email">ชื่อผู้ใช้งาน</label>
                                <input type="text" class="form-control" name='username'>
                            </div>
                            <div class="form-group">
                                <label for="password">รหัสผ่าน</label>
                                <input type="password" class="form-control" name='password'>
                            </div>
                            <div class="text-center">
                                <button type="submit" class="btn btn-default">ลงชื่อเข้าใช้</button>
                            </div>
                        </form>

                    </div>

                </div>
            </div>

