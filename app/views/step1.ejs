                                    </div>
                                    <div class="col-md-2">
                                        <button class="search-btn" type="submit" data-wow-delay="0.45s">รอบฉาย</button>
                                    </div>
                                </form>
                            </div>

                            <%theatres.forEach(function(th){%>
                            <div class="col-sm-11 panel-group padding-top-25">
                                <div class="panel panel-default">
                                    <div class="panel-heading">
                                        <h4 class="panel-title  fqa-title">
                                            <%=th.theatrename%>
                                        </h4>
                                    </div>
                                    <!-- ลูปตรงนี้นาจา -->
                                    
                                    <div class=" fqa-body panel-body">
                                            <%th.shwowtime.forEach(function(sh){%>
                                            <%if(parseFloat(sh)>14){%>
                                                <%if(username){%>
                                        <a class="btn btn-primary btn-next " href=/payment2/<%=th.branchname%>/<%=th.theatrenum%>/<%=sh%>><%=sh%></a>
                                        <%}else{%>
                                            <a class="btn btn-primary btn-next " href=/register/index><%=sh%></a>
                                        <%}%>
                                        <%}else{%>
                                        <a class="btn btn-primary btn-next " ><%=sh%></a>
                                         <%}%>
                                        <%})%>
                                    </div>
                                    
                                </div>
                            </div>
                            <%})%>
                        </div>
                    </div>
